{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Loc_Cliente",
						"type": "DatasetReference"
					},
					"name": "Cliente"
				},
				{
					"name": "Agencia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dim_Cliente_DW",
						"type": "DatasetReference"
					},
					"name": "DimCliente",
					"rejectedDataLinkedService": {
						"referenceName": "DLIngestion",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Cd_cliente as integer,",
				"          Nm_Cliente as string,",
				"          Cd_Tp_Cliente as integer,",
				"          Nr_Estrelas as integer,",
				"          Nr_Habilitacao as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Cliente",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Agencia",
				"Cliente select(mapColumn(",
				"          Cd_cliente,",
				"          Nm_Cliente,",
				"          Cd_Tp_Cliente,",
				"          Nr_Estrelas,",
				"          Nr_Habilitacao",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> DimCliente"
			]
		}
	}
}